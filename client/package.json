{
  "name": "pocket-goblin",
  "version": "1.0.0",
  "description": "React client for Pocket Goblin",
  "main": "index.js",
  "scripts": {
    "copy:media": "cp -R src/images dist/",
    "start": "parcel src/index.html",
    "format": "prettier --write \"src/**/*.{js,jsx}\"",
    "format:check": "prettier --list-different \"src/**/*.{js,jsx,css,json}\"",
    "lint": "eslint \"**/*.{js,jsx}\" --quiet",
    "build": "parcel build src/index.html --no-source-maps ",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/deduced/pocket-goblin-client.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/deduced/pocket-goblin-client/issues"
  },
  "homepage": "https://github.com/deduced/pocket-goblin-client#readme",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "eslint": "^5.6.1",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-prettier": "^2.7.0",
    "eslint-plugin-react": "^7.11.1",
    "parcel-bundler": "^1.10.1"
  },
  "dependencies": {
    "auth0-js": "^9.8.0",
    "axios": "^0.18.0",
    "chart.js": "^2.7.2",
    "emotion": "^9.2.12",
    "history": "^4.7.2",
    "react": "^16.5.2",
    "react-chartjs-2": "^2.7.4",
    "react-dom": "^16.5.2",
    "react-emotion": "^9.2.12",
    "react-plaid-link": "^1.0.2",
    "react-router-dom": "^4.3.1"
  }
}
